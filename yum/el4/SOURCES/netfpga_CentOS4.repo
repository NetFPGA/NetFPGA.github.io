[netfpga]
name=NetFPGA repository for CentOS4 - $basearch
baseurl=http://netfpga.org/yum/el4
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-NETFPGA

[netfpga-source]
name=NetFPGA repository for CentOS4 - Source
baseurl=http://netfpga.org/yum/el4
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-NETFPGA
